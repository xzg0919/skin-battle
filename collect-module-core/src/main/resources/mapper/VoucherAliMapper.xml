<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tzj.collect.core.mapper.VoucherAliMapper">
<update id="updatePickCount" parameterType="Long">
	UPDATE sb_voucher_ali
	SET pick_count = pick_count + 1
	WHERE id = #{voucherId}
</update>








</mapper>