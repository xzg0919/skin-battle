<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tzj.green.mapper.MemberCardMapper">


<insert id="addCard" parameterType="java.util.List">

	INSERT INTO sb_member_card (
	member_card,
	create_date,
	update_date
	)
	VALUES
	<foreach collection="csvList" item="item" index="index" separator=",">
		(#{item},now(),now())
	</foreach>


</insert>








</mapper>
